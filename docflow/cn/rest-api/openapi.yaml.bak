                                        $ref: '#/components/schemas/ReviewTaskStatus'
                                      reasoning:
                                        type: string
                                        description: 审核依据
                                        example: "审核依据"
                                      anchors:
                                        type: array
                                        description: 审核依据的位置回溯信息，用于定位审核依据在原文中的位置
                                        items:
                                          type: object
                                          properties:
                                            start_pos:
                                              type: integer
