            "group": "智能文档解析",
            "pages": [
              "xparse/extract-quickstart",
              "xparse/extract-getjson",
              "xparse/extract-gettable",
              "xparse/extract-getpos",
              "xparse/extract-asyncio",
              "xparse/extract-max-concurrent"
            ]
          }
        ]
      }
